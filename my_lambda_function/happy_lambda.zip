resource "aws_lambda_function" "my_lambda_function" {
  function_name = "MyGreetingLambda"
  handler       = "index.handler"
  runtime       = "python3.8"
  role          = aws_iam_role.lambda_exec_role.arn
  filename      = "happy_lambda.zip"
  environment {
    variables = {
      SOME_ENV_VAR = "value"
    }
  }
}